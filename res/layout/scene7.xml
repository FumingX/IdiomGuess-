<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        android:src="@drawable/d7" />

    <Button
        android:id="@+id/level73"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="42dp" />

    <Button
        android:id="@+id/level74"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/level73"
        android:layout_centerVertical="true"
        android:layout_marginLeft="41dp" />

    <Button
        android:id="@+id/level75"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/level73"
        android:layout_alignBottom="@+id/level73"
        android:layout_marginLeft="32dp"
        android:layout_toRightOf="@+id/level74" />

    <Button
        android:id="@+id/level76"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/level75"
        android:layout_below="@+id/level74"
        android:layout_marginLeft="20dp" />

    <Button
        android:id="@+id/level77"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/level75"
        android:layout_marginLeft="49dp"
        android:layout_marginTop="14dp"
        android:layout_toRightOf="@+id/level76" />

    <Button
        android:id="@+id/level78"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/level77"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="20dp" />

    <Button
        android:id="@+id/level79"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/level77"
        android:layout_alignBottom="@+id/level77"
        android:layout_alignParentRight="true"
        android:layout_marginRight="22dp" />

    <Button
        android:id="@+id/level80"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/level76"
        android:layout_marginLeft="22dp"
        android:layout_toRightOf="@+id/level78" />

    <Button
        android:id="@+id/level81"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/level77"
        android:layout_alignBottom="@+id/level77"
        android:layout_toLeftOf="@+id/level77" />

    <Button
        android:id="@+id/level82"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="17dp"
        android:layout_toRightOf="@+id/level76" />

    <Button
        android:id="@+id/level83"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/level82"
        android:layout_toLeftOf="@+id/level76" />

    <Button
        android:id="@+id/level84"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/level83"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="22dp" />

</RelativeLayout>
